<h1>Sign In</h1>

<div>
  <%= form_for(:session, url: login_path) do |f| %>

    <%= f.label :email %>
    <%= f.text_field :email %>

    <%= f.label :password %>
    <%= f.password_field :password %>

    <%= f.submit "Sign In" %>
  <% end %>

  <p>New user? <%= link_to "Sign up now!", new_user_path %></p>
</div>